{
  "name": "BYOND NightMode",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Sets custom CSS styling to the BYOND website.",

  "content_scripts": [{
    "js": ["js/jquery-2.1.3.min.js", "js/custom.js"],
    "css": ["css/style.css"],
    "matches": ["https://*.byond.com/*", "http://*.byond.com/*"]
  }]
}